{
  "name": "mandecoisas-api",
  "version": "1.4.0",
  "description": "Node starter template with TypeScript",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/leodiegoo/mandecoisas-api.git"
  },
  "keywords": [],
  "author": "leodiegoo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leodiegoo/mandecoisas-api/issues"
  },
  "homepage": "https://github.com/leodiegoo/mandecoisas-api#readme",
  "files": [
    "dist/src/*"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf coverage dist docs",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "debug": "node --nolazy --inspect-brk=9229 node_modules/jest/bin/jest.js -i",
    "dev": "tsnd --inspect --transpile-only --respawn --ignore-watch node_modules src/server.ts",
    "docs": "typedoc --tsconfig ./tsconfig.json src && shx cp .nojekyll docs/.nojekyll",
    "format": "prettier --ignore-path .eslintignore --write \"**/*.{js,ts,jsx,tsx}\" -c",
    "lint": "eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublishOnly": "npm run test && npm run lint",
    "release:alpha": "npm version prerelease --preid alpha -m \" build: :bookmark:%s\"",
    "release:beta": "npm version prerelease --preid beta -m \"build: :bookmark: %s\"",
    "release:patch": "npm version patch -m \"build: :bookmark: %s\"",
    "release:minor": "npm version minor -m \"build: :bookmark: %s\"",
    "release:major": "npm version major -m \"build: :bookmark: %s\"",
    "start": "node ./dist/src/server.js",
    "setup": "npm install && ts-node ./setup.ts",
    "test": "jest --coverage -t",
    "test:watch": "jest --watch-all -t",
    "version": "git add -A src"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.3.0",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "date-fns": "^2.16.1",
    "date-fns-tz": "^1.0.10",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "fast-crc32c": "^2.0.0",
    "firebase": "^7.21.0",
    "firebase-admin": "^9.2.0",
    "helmet": "^4.1.1",
    "lodash": "^4.17.20",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-cloud-storage": "^2.4.0",
    "rangen": "^1.0.0",
    "tslib": "^2.0.1",
    "uuid": "^8.3.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^10.0.0",
    "@types/cors": "^2.8.7",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.8",
    "@types/git-repo-name": "^1.0.0",
    "@types/git-username": "^1.0.0",
    "@types/jest": "^26.0.10",
    "@types/lodash": "^4.14.160",
    "@types/morgan": "^1.9.1",
    "@types/multer": "^1.4.4",
    "@types/promptly": "^3.0.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "commitlint": "^9.1.2",
    "coveralls": "^3.1.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "git-repo-name": "^1.0.1",
    "git-repo-url": "^0.1.0",
    "git-username": "^1.0.0",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "jest-extended": "^0.11.5",
    "lint-staged": "^10.2.13",
    "minimist": "^1.2.5",
    "pkg-homepage": "^0.1.1",
    "prettier": "^2.1.1",
    "promptly": "^3.0.3",
    "rimraf": "^3.0.2",
    "shx": "^0.3.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.62",
    "typedoc": "^0.18.0",
    "typescript": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "jest --bail --findRelatedTests"
    ]
  }
}
